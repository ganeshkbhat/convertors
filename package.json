    "multi-ini": "^2.3.2",
    "pdf_neutralize": "^1.0.0",
    "pdf-node": "^1.0.1",
    "pdf2html": "^3.1.0",
    "pdfjs-dist": "^4.0.379",
    "pdfkit": "^0.14.0",
    "postcss": "^8.4.32",
    "v8": "^0.1.0",
    "xml": "^1.0.1",
    "xml2js": "^0.6.2",
    "yamljs": "^0.3.0"
  }
