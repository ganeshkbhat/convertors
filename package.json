  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "scripts": {
    "pdfdeps": "sudo apt install -y python python3 build-essential ghostscript libjpeg-dev libpng-dev libcurl4-openssl-dev mupdf-tools libfreetype6-dev qpdf",
    "test": "mocha --reporter spec --recursive --timeout 60000",
    "setupdevenv": "npm install -g mocha",
    "release": "release-it"
  },
